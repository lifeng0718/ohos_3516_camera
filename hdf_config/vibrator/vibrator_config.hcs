root {
    vibratorConfig {
        boardConfig {
            match_attr = "hdf_vibrator_driver";
            vibratorAttr {
                /* 0:rotor 1:linear */
                deviceType = 1;
                supportPreset = 1;
            }
            vibratorHapticConfig {
                haptic_clock_timer = [1, 600, 600, 200, 600]; // time seq
                haptic_default_effect = [0, 0, 3, 800, 1]; // build-in effect
            }
        }
    }
}
